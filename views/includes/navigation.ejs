<nav class="navbar navbar-dark bg-secondary">
  <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#collapsingNavbar">
    <span class="dark-blue-text"><i
      class="fas fa-bars fa-1x"></i></span>
  </button>
  <a class="navbar-brand" href="/integration/testcases">INTEGRATION</a>
  <div class="navbar-collapse collapse" id="collapsingNavbar">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link"  href="/integration/testcases">Home</a>
        </li>
         <% if(path !== '/integration/testCases/new' && access == 1) { %>
          <li class="nav-item">
            <a class="nav-link" href="/integration/testCases/new">New Test</a>
          </li>
          <% } %>
          <% if(path !== '/integration/user/new' && access == 1) { %>
          <li class="nav-item">
            <a class="nav-link"  href="/integration/user/new">Create New User</a>
          </li>
          <% } %>
          <% if(path !== '/integration/user/' + email + '/edit') { %>
          <li class="nav-item">
            <a class="nav-link" href="/integration/user/<%= email %>/edit">Change Password</a>
          </li>
          <% } %>
          
          <li class="nav-item mt-2">
            <form action="/integration/logout" method="POST">
              <button type="submit" class="btn btn-link nav-link">LOG OUT</button>
            </form>
          </li>
          
      </ul>
  </div>
</nav>